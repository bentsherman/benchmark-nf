manifest {
	mainScript = "main.nf"
	defaultBranch = "master"
	nextflowVersion = ">=19.04.0"
}



params {
	input {
		dir = "kinc/output"
		trace_files = "reports/trace.txt"
		nvprof_files = "*.nvprof.txt"
	}

	output {
		dir = "output"
	}

	aggregate {
		enabled = true
		nvprof_mapper = "${baseDir}/files/nvprof-mapper.txt"
	}

	visualize {
		enabled = true
		format = "png"
		plots = [
			[
				"xaxis": "experiment_value",
				"yaxis": "nvprof_time_avg",
				"hue1": "gpu_model",
				"hue2": ""
			]
		]
	}
}



profiles {
	standard {
		process {
			executor = "local"
			cpus = 1
			memory = "8 GB"
		}
		executor {
			queueSize = 1
		}
	}
}
